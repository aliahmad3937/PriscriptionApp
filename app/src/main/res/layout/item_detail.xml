<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
   android:layout_margin="12dp"
    app:cardCornerRadius="20dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="15dp"
        >


        <TextView
            android:id="@+id/tv_rxnumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="TextView"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="5dp"
            android:background="#878181"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_rxnumber" />

        <TextView
            android:id="@+id/tv_l"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:text="Dispensed Item Name"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@+id/textView8"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view3" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:layout_marginHorizontal="@dimen/mrg_hor_coln"
            app:layout_constraintEnd_toStartOf="@+id/tv_location"
            app:layout_constraintTop_toBottomOf="@+id/view3"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_l"
            tools:layout_editor_absoluteY="107dp" />

        <TextView
            android:id="@+id/tv_location"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Location Address"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/view3" />





        <TextView
            android:id="@+id/tv_d"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:text="Date Filled"
            app:layout_constraintEnd_toStartOf="@+id/textView9"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_location" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:layout_marginHorizontal="@dimen/mrg_hor_coln"
            app:layout_constraintEnd_toStartOf="@+id/tv_date"
            app:layout_constraintTop_toBottomOf="@+id/tv_location"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_d"
            tools:layout_editor_absoluteY="107dp" />

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="0dp"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_height="wrap_content"
            android:text="Location Address"
            android:layout_marginVertical="@dimen/mrg_vert"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            app:layout_constraintTop_toBottomOf="@+id/tv_location" />




        <TextView
            android:id="@+id/tv_rx"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:text="Rx Status"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@+id/textView10"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_date" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:layout_marginHorizontal="@dimen/mrg_hor_coln"
            app:layout_constraintEnd_toStartOf="@+id/tv_rxstatus"
            app:layout_constraintTop_toBottomOf="@+id/tv_date"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_rx"
            tools:layout_editor_absoluteY="107dp" />

        <TextView
            android:id="@+id/tv_rxstatus"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Location Address"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/tv_date" />





        <TextView
            android:id="@+id/tv_q"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:text="Quantity"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@+id/textView11"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_rxstatus" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textColor="@color/black"
            android:fontFamily="@font/poppins_semibold"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:layout_marginHorizontal="@dimen/mrg_hor_coln"
            app:layout_constraintEnd_toStartOf="@+id/tv_quantity"
            app:layout_constraintTop_toBottomOf="@+id/tv_rxstatus"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_q"
            tools:layout_editor_absoluteY="107dp" />

        <TextView
            android:id="@+id/tv_quantity"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Completed"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/tv_rxstatus" />



        <TextView
            android:id="@+id/tv_t"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:text="Current Transaction Status"
            app:layout_constraintEnd_toStartOf="@+id/textView12"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_quantity" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:layout_marginHorizontal="@dimen/mrg_hor_coln"
            app:layout_constraintEnd_toStartOf="@+id/tv_transaction_status"
            app:layout_constraintTop_toBottomOf="@+id/tv_quantity"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_t"
            tools:layout_editor_absoluteY="107dp" />

        <TextView
            android:id="@+id/tv_transaction_status"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Location Address"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/tv_quantity" />



        <TextView
            android:id="@+id/tv_refill"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:text="Refill or New"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@+id/textView13"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_t" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            android:layout_marginHorizontal="@dimen/mrg_hor_coln"
            app:layout_constraintEnd_toStartOf="@+id/tv_refill_new"
            app:layout_constraintTop_toBottomOf="@+id/tv_t"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/tv_refill"
            tools:layout_editor_absoluteY="107dp" />

        <TextView
            android:id="@+id/tv_refill_new"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Location Address"
            android:fontFamily="@font/poppins_semibold"
            android:textColor="@color/black"
            android:layout_marginVertical="@dimen/mrg_vert"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/tv_t" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>





